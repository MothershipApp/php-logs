{
    "name": "mothership-app/php-logs",
    "description": "Error logging library for Mothership.app",
    "keywords": [
        "logs",
        "logging",
        "mothership",
        "debugging",
        "exceptions",
        "errors"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Mothership Team",
            "email": "support@mothership.app"
        }
    ],
    "require": {
        "php": ">=8.0.0 <9.0",
        "ext-curl": "*",
        "psr/log": "^1 || ^2 || ^3",
        "monolog/monolog": "^2 || ^3"
    },
    "autoload": {
        "psr-4": {
            "Mothership\\": "src/"
        }
    },
    "config": {
        "process-timeout": 600
    }
}
